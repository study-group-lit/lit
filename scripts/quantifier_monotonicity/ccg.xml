<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>

<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="lex" cat="NP">
   <rule type="fa" cat="N">
    <lf start="0" span="1" word="@" lemma="@" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
    <lf start="1" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="2" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
   <rule type="fa" cat="NP[nb]">
    <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <rule type="fa" cat="N/N">
      <lf start="4" span="1" word="most" lemma="most" pos="RBS" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
      <lf start="5" span="1" word="successful" lemma="successful" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     </rule>
     <lf start="6" span="1" word="snowboarder" lemma="snowboarder" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="ba" cat="NP">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="O'Neill" lemma="O'Neill" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="1" span="1" word=":" lemma=":" pos="IN" chunk="O" entity="O" cat="(NP\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="judge" lemma="judge" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="@" lemma="@" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="7" span="1" word="now" lemma="now" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="8" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="9" span="1" word="legislative" lemma="legislative" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="10" span="1" word="background" lemma="background" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="lex" cat="NP">
   <rule type="fa" cat="N">
    <lf start="0" span="1" word="three" lemma="three" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
    <rule type="lp" cat="N">
     <lf start="1" span="1" word="-" lemma="-" pos=":" chunk="I-NP" entity="I-DAT" cat=":" />
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="I-DAT" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="winner" lemma="winner" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="4" span="1" word="@" lemma="@" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="5" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="6" span="1" word="believes" lemma="believe" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="ba" cat="NP">
     <lf start="7" span="1" word="any" lemma="any" pos="DT" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="NP\NP">
      <lf start="8" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="10" span="1" word="top" lemma="top" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="11" span="1" word="four" lemma="four" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="12" span="1" word="players" lemma="player" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="13" span="1" word="could" lemma="could" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
     <lf start="14" span="1" word="triumph" lemma="triumph" pos="VB" chunk="I-NP" entity="O" cat="S[b]\NP" />
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="NP">
  <rule type="lex" cat="NP">
   <rule type="fa" cat="N">
    <lf start="0" span="1" word="Arun" lemma="Arun" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
    <lf start="1" span="1" word="Kundnani" lemma="Kundnani" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
  </rule>
  <rule type="fa" cat="NP\NP">
   <lf start="2" span="1" word=":" lemma=":" pos="IN" chunk="O" entity="O" cat="(NP\NP)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <lf start="3" span="1" word="some" lemma="some" pos="DT" chunk="I-NP" entity="O" cat="NP" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="urge" lemma="urge" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="U.S." lemma="U.S." pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="7" span="1" word="government" lemma="government" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="8" span="1" word="program" lemma="program" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="lex" cat="NP\NP">
       <rule type="fa" cat="S[pss]\NP">
        <lf start="9" span="1" word="aimed" lemma="aim" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
        <rule type="fa" cat="PP">
         <lf start="10" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="11" span="1" word="extreme" lemma="extreme" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <rule type="fa" cat="N">
            <lf start="12" span="1" word="@" lemma="@" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
            <rule type="fa" cat="N">
             <lf start="13" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="14" span="1" word="views" lemma="view" pos="NNS" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="owner" lemma="owner" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="this" lemma="this" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="estate" lemma="estate" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="6" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="7" span="1" word="ordinary" lemma="ordinary" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="8" span="1" word="Lord" lemma="Lord" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="9" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="I-LOC" cat="NP[nb]/N" />
       <lf start="11" span="1" word="Manor" lemma="Manor" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="fa" cat="S[X]\S[X]">
   <lf start="12" span="1" word="--" lemma="--" pos=":" chunk="O" entity="O" cat="(S\S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <lf start="13" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="14" span="1" word="'s" lemma="'s" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="15" span="1" word="@" lemma="@" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="16" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="17" span="1" word="tycoon" lemma="tycoon" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="18" span="1" word="Max" lemma="Max" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="lp" cat="S[dcl]">
  <lf start="0" span="1" word="``" lemma="``" pos="LQU" chunk="O" entity="O" cat="LQU" />
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="1" span="1" word="this" lemma="this" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="2" span="1" word="wonderful" lemma="wonderful" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="3" span="1" word="couple" lemma="couple" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="4" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="6" span="1" word="danger" lemma="danger" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="7" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="8" span="1" word="no" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="9" span="1" word="one" lemma="one" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
     <lf start="10" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
     <rule type="lp" cat="S[dcl]\NP">
      <lf start="11" span="1" word="``" lemma="``" pos="LQU" chunk="O" entity="O" cat="LQU" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="12" span="1" word="writes" lemma="write" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="13" span="1" word="@" lemma="@" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="14" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="S[X]/S[X]">
  <lf start="0" span="1" word="while" lemma="while" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="1" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
    <rule type="lex" cat="NP">
     <lf start="2" span="1" word="prison" lemma="prison" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="lp" cat="S[dcl]">
    <lf start="3" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="Mandela" lemma="Mandela" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="5" span="1" word="became" lemma="become" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="most" lemma="most" pos="RBS" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="7" span="1" word="significant" lemma="significant" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <rule type="fa" cat="N">
           <lf start="8" span="1" word="black" lemma="black" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="9" span="1" word="leader" lemma="leader" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="10" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="11" span="1" word="@" lemma="@" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
          <lf start="12" span="1" word="placeholder" lemma="placeholder" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>

</candc>
